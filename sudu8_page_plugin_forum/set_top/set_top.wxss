page {
    background: #fff;
}

.hbj {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.flex1 {
    flex: 1;
}

.set_top_money {
    height: 88rpx;
    border-bottom: 2rpx solid #EEEEEE;
    padding: 0 30rpx;
}

.set_top_left {
    font-size: 28rpx;
    color: #434343;
}

.set_top_right {
    font-size: 14rpx;
    color: #838383;
}

.set_top_right text {
    font-family: Arial,Helvetica,sans-serif;
    font-size: 28rpx;
    color: #838383;
}

.set_top_day {
    font-size: 28rpx;
    color: #838383;
}

.set_top_input {
    border: 0;
    padding: 0;
    margin: 0;
    font-size: 24rpx;
    color: #c4c4c4;
    text-align: right;
}

.allmoney {
    padding: 30rpx 30rpx 0rpx 30rpx;
}

.allmoney_left {
    font-size: 28rpx;
    color: #2f74fd;
}

.allmoney_right {
    font-size: 14rpx;
    color: #2f74fd;
}

.allmoney_right text {
    font-family: Arial,Helvetica,sans-serif;
    font-size: 28rpx;
}

.relase_btn {
    margin: 40rpx auto 0;
    width: 440rpx;
    height: 80rpx;
    text-align: center;
    border-radius: 80rpx;
    background-color: #2F74FD;
    line-height: 80rpx;
    color: #fff;
    font-size: 28rpx;
}