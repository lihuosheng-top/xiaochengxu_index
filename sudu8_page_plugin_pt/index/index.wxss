.swiper_container {
    height: 55rpx;
    width: 460rpx;
    background-color: rgba(0,0,0,0.5);
    border-radius: 55rpx;
}

view {
    overflow: visible;
}

.swiper_item {
    height: 55rpx;
    line-height: 55rpx;
    display: flex;
    flex-direction: row;
}

.swiper_item_image {
    width: 55rpx;
    height: 55rpx;
    border-radius: 55rpx;
}

.swiper_item_text {
    width: 395rpx;
    height: 55rpx;
    line-height: 55rpx;
    font-size: 24rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    letter-spacing: 2px;
    color: #fff;
    margin-left: 10rpx;
}

.goods_list1 {
    padding: 30rpx 20rpx;
    background-color: #fff;
    display: flex;
    flex-direction: row;
    position: relative;
    margin-bottom: 2%;
}

.goods_list1_image {
    width: 250rpx;
    height: 250rpx;
}

.goods_list1_info {
    width: 440rpx;
    margin-left: 20rpx;
    display: flex;
    flex-direction: column;
}

.goods_list1_info_title {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #000;
    font-size: 30rpx;
}

.goods_list1_info_intro {
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #666;
    font-size: 28rpx;
    overflow: hidden;
}

.goods_list1_info_count {
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #666;
    font-size: 26rpx;
    margin-top: 10rpx;
}

.goods_list1_info_buy {
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #999;
    font-size: 26rpx;
    margin-top: 30rpx;
}

.goods_list1_info_buy text {
    text-decoration: line-through;
}

.goods_list1_info_tuan {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-size: 26rpx;
}

.goods_list1_info_tuan text {
    font-size: 46rpx;
    font-weight: 700;
}

.goods_list1_button {
    position: absolute;
    width: 175rpx;
    height: 70rpx;
    bottom: 30rpx;
    right: 20rpx;
    border-radius: 6rpx;
    background-color: #ff4a49;
    color: #fff;
    font-size: 32rpx;
    line-height: 70rpx;
    text-align: center;
}

.goods_list2_box {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
}

.goods_list2 {
    display: flex;
    flex-direction: column;
    width: 355rpx;
    margin: 0 10rpx;
    padding-bottom: 10rpx;
    position: relative;
}

.goods_list2_image {
    width: 355rpx;
    height: 355rpx;
}

.goods_list2_title {
    font-size: 26rpx;
    color: #000;
    padding-left: 20rpx;
    width: 335rpx;
    margin-top: 10rpx;
}

.goods_list2_info {
    width: 315rpx;
    padding: 0 20rpx;
    margin-top: 20rpx;
    display: flex;
    flex-direction: row;
}

.goods_list2_info_price {
    width: 115rpx;
    font-size: 26rpx;
}

.goods_list2_info_price text {
    font-size: 36rpx;
    font-weight: 700;
}

.goods_list2_info_count {
    width: 200rpx;
    text-align: right;
    font-size: 26rpx;
    color: #999;
    margin-top: 10rpx;
}

.goods_list3 {
    padding: 20rpx;
    background-color: #fff;
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: flex-start;
}

.goods_list3_xsms {
    font-size: 30rpx;
    color: pink;
}

.goods_list3_top {
    display: flex;
    flex-direction: row;
    align-items: center;
    position: relative;
}

.swiper_container2 {
    height: 44rpx;
    width: 300rpx;
    line-height: 44rpx;
    margin-left: 5rpx;
}

.swiper_item2 {
    font-size: 24rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #666;
}

.goods_list3_top_djs {
    position: absolute;
    top: 0;
    right: 0;
    display: flex;
    flex-direction: row;
    align-items: center;
}

.goods_list3_top_djs_text {
    font-size: 26rpx;
    color: #666;
}

.goods_list3_top_djs_hr {
    width: 35rpx;
    height: 35rpx;
    font-size: 22rpx;
    text-align: center;
    background-color: #333;
    line-height: 35rpx;
    color: #fff;
    border-radius: 6rpx;
    margin-left: 5rpx;
}

.goods_list3_dg {
    display: flex;
    flex-direction: column;
    margin: 0 2% 3% 0;
}

.goods_list3_dg_image {
    width: 220rpx;
    height: 210rpx;
    border-radius: 10rpx;
}

.goods_list3_dg_bottom {
    display: flex;
    flex-direction: row;
    margin-top: 10rpx;
    padding: 0 10rpx;
    align-items: center;
}

.goods_list3_msj {
    width: 100rpx;
    height: 35rpx;
    font-size: 24rpx;
    text-align: center;
    line-height: 35rpx;
    border-radius: 6rpx;
}

.goods_list3_price {
    margin-left: 5rpx;
    font-size: 30rpx;
}

.ptry {
    padding: 20rpx;
    display: flex;
    flex-direction: column;
    background-color: #fff;
}

.ptry_image {
    width: 710rpx;
    position: relative;
}

.ptry_image_view1 {
    width: 100%;
    background-color: rgba(0,0,0,0.5);
    color: #fff;
    font-size: 28rpx;
    position: absolute;
    bottom: 0;
    left: 0;
    height: 66rpx;
    line-height: 66rpx;
    padding: 0 20rpx;
    overflow: hidden;
}

.ptry_image_view2 {
    text-align: center;
    border-bottom-right-radius: 30rpx;
    border-top-right-radius: 30rpx;
    height: 55rpx;
    line-height: 55rpx;
    color: #fff;
    position: absolute;
    bottom: 85rpx;
    left: 0;
    padding: 0 50rpx 0 40rpx;
    background-color: rgba(0,0,0,0.5);
}

.ptry_imggroup {
    display: flex;
    flex-direction: row;
    margin-top: 20rpx;
}

.ptry_imggroup_image {
    width: 60rpx;
    height: 60rpx;
    margin-right: -18rpx;
    border-radius: 50rpx;
    border: 4rpx solid #fff;
}

.ptry_text {
    font-size: 26rpx;
    margin-top: 20rpx;
}

.ptry_text span {
    float: left;
    color: #fff;
    border-radius: 6rpx;
    font-size: 24rpx;
    padding: 0 10rpx;
    margin-right: 10rpx;
}

.ptry_text text {
    padding: 0 10rpx;
}

.slide-image {
    width: 100%;
}

.binder {
    position: relative;
}

.binder_view {
    position: absolute;
    left: 0;
    bottom: 0;
    background-color: #f34f2c;
    width: 710rpx;
    height: 90rpx;
    z-index: 10000;
    padding: 0 20rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
}

.binder_view_left {
    color: #fff;
    height: 90rpx;
    line-height: 80rpx;
    font-size: 26rpx;
}

.binder_view_left text {
    font-size: 46rpx;
}

.binder_view_right {
    margin-left: 140rpx;
}

.binder_view_right_djs {
    display: flex;
    flex-direction: row;
}

.binder_view_right_text {
    font-size: 24rpx;
    color: #fff;
}

.binder_right_text {
    font-size: 24rpx;
    color: #fff;
}

.binder_hr {
    width: 30rpx;
    height: 30rpx;
    font-size: 22rpx;
    text-align: center;
    background-color: #fff;
    line-height: 30rpx;
    color: #f34f2c;
    border-radius: 6rpx;
    margin-left: 5rpx;
}

.drct {
    padding: 20rpx;
    background-color: #fff;
}

.drct_top {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.drct_top_center {
    padding: 0 10rpx;
    height: 35rpx;
    line-height: 35rpx;
    font-size: 20rpx;
    border: 2rpx solid #f34f2c;
    background-color: #f6bcb0;
    color: #f34f2c;
    margin-left: 5rpx;
}

.drct_top_right {
    flex: 1;
    text-align: right;
}

.drct_bottom {
    margin-top: 20rpx;
    display: flex;
    flex-direction: row;
    margin-bottom: 15rpx;
}

.drct_bottom_view {
    width: 167rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    border: 2rpx solid #f34f2c;
    border-radius: 10rpx;
    margin-right: 20rpx;
}

.drct_bottom_view_text {
    font-size: 24rpx;
    color: #f34f2c;
}

.drct_bottom_view_on {
    background-color: #f34f2c;
}

.drct_bottom_view_text_on {
    color: #fff;
}

.grzxh {
    padding: 30rpx 0;
    display: flex;
    flex-direction: row;
    background-color: #fff;
}

.grzxh_view {
    width: 20%;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.grzxh_view_img {
    width: 60rpx;
}