page {
    background: #fff;
}

.hbj {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.flex1 {
    flex: 1;
}

.prizebox {
    padding: 0 30rpx;
}

.prize_single {
    padding: 50rpx 0;
    border-bottom: 2rpx solid #eeeeee;
}

.prize_left {
    border-radius: 12rpx;
    overflow: visible;
    position: relative;
}

.prize_single_left {
    width: 124rpx;
    height: 124rpx;
    border-radius: 12rpx;
    display: block;
}

.ydjbg {
    width: 44rpx;
    height: 44rpx;
    position: absolute;
    right: -22rpx;
    top: -22rpx;
}

.ydjbg view {
    text-align: center;
    color: #5a2300;
    font-size: 22rpx;
    position: absolute;
    left: 12rpx;
    top: 12rpx;
    font-family: Arial,Helvetica,sans-serif;
}

.prize_single_right {
    width: 530rpx;
    margin-left: 36rpx;
}

.prize_title {
    font-size: 30rpx;
    color: #434343;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    height: 44rpx;
    line-height: 44rpx;
}

.prize_count {
    margin-top: 20rpx;
}

.prize_count_view {
    font-size: 24rpx;
    color: #838383;
}