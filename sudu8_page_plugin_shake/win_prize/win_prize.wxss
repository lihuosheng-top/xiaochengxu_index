page {
    background-color: #fff;
}

.hbj {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.flex1 {
    flex: 1;
}

.win_prize_box {
    padding: 0 30rpx;
}

.win_prize_single {
    height: 204rpx;
    border-bottom: 2rpx solid #eee;
}

.win_price_tx {
    width: 124rpx;
    height: 124rpx;
    border-radius: 50%;
}

.win_price_right {
    margin-left: 40rpx;
    width: 526rpx;
}

.win_prize_name {
    max-width: 350rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    font-size: 30rpx;
}

.win_price_num {
    font-size: 24rpx;
    color: #434343;
    margin-top: 4rpx;
    margin-left: 10rpx;
}

.jpmc {
    font-size: 24rpx;
    color: #838383;
    width: 270rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.win_price_time {
    font-size: 24rpx;
    color: #838383;
}

.flex1 {
    flex: 1;
}