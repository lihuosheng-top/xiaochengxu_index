.multiCate {
    text-align: center;
    height: 80rpx;
    line-height: 80rpx;
    position: relative;
    overflow: visible;
}

.multiCateOne {
    float: left;
    overflow: visible;
    line-height: 80rpx;
}

.multiCateOne i {
    border-color: #333;
    display: inline-block;
    position: relative;
    top: -6rpx;
    left: 6rpx;
    width: 0;
    height: 0;
    line-height: 0;
    border-bottom: 0;
    border-left: 10rpx solid transparent;
    border-right: 10rpx solid transparent;
    border-top: 10rpx solid #ccc;
}

.multiCateOne i.on {
    border-left: 10rpx solid transparent;
    border-right: 10rpx solid transparent;
}

.multiCateSub {
    background: rgba(0,0,0,0.7);
    text-align: center;
    padding: 0 2%;
    position: absolute;
    left: 0;
    width: 100%;
    height: auto;
    top: 90rpx;
    z-index: 999;
    box-sizing: border-box;
    padding-top: 26rpx;
}

.multiCateSub view {
    float: left;
    height: 50rpx;
    line-height: 50rpx;
    margin: 0 2% 26rpx;
    width: 29.33%;
    background: #fff;
    color: #333;
    font-size: 26rpx;
    border-radius: 6rpx;
}