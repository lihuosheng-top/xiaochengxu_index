page {
    color: #454545;
    font-size: 28rpx;
}

image {
    overflow: visible;
}

.b-fff {
    background: #ffffff;
}

.mb20 {
    margin-bottom: 20rpx;
}

.mr10 {
    margin-right: 10rpx;
}

.pd10 {
    padding: 10rpx;
}

.flex-row {
    display: flex;
    flex-direction: row;
}

.flex-shu-center {
    align-items: center;
}

.color_red {
    color: #f03150;
}

input {
    border: 0;
    padding: 0;
    margin: 0;
}

.f-bold {
    font-weight: bold;
}

.main {
    background: #ffffff;
}

.fxtop {
    background: linear-gradient(120deg,#FE593D,#FF204D);
    height: 220rpx;
    position: relative;
    display: flex;
    flex-direction: row;
    align-items: center;
}

.fxtop .con {
    z-index: 100001;
    width: 700rpx;
    margin: 0 auto;
    display: flex;
    flex-direction: row;
    align-items: center;
    color: #ffffff;
    position: absolute;
    left: 40rpx;
}

.touxiang {
    width: 120rpx;
    height: 120rpx;
    background: #fff;
    margin-right: 20rpx;
    border-radius: 50%;
    overflow: hidden;
}

.touxiang image {
    width: 120rpx;
    height: 120rpx;
}

.nickname {
    font-size: 34rpx;
}

.middle {
    height: 170rpx;
    padding: 0 20px;
    background-color: #fff;
    border-bottom: 2rpx solid #f6f6f9;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.yongjin {
    display: flex;
    flex-direction: column;
}

.yongjin>view {
    height: 142rpx;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.yongjin>view>view {
    width: 374rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.yongjin>view>view:nth-child(2n+1) {
    border-right: 2rpx solid #f6f6f9;
}

.yongjin>view:nth-child(1) {
    border-bottom: 2rpx solid #f6f6f9;
}

.tixian_sub {
    width: 200rpx;
    border: 2rpx solid #f23030;
    border-radius: 10rpx;
    margin: 30rpx auto;
    color: #f23030;
    text-align: center;
    padding: 10rpx 0;
}

.line {
    background: #f1f1f1;
    height: 20rpx;
}

.mingxi {
    border-top: 2rpx solid #eee;
    border-bottom: 2rpx solid #eee;
    background: #fff;
}

.mingxi>view {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    height: 100rpx;
    color: #222;
    border-bottom: 2rpx solid #F6F6F9;
    padding: 0 20rpx;
}