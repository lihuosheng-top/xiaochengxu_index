page {
    background-color: #F6F6F6;
}

.hbj {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.card_listbox {
    background-color: #fff;
    border-radius: 12rpx;
    width: 710rpx;
    margin: 30rpx auto 0;
}

.card_list_head {
    height: 145rpx;
    padding: 0 24rpx;
    border-bottom: 2rpx solid #eee;
}

.card_list_head_left {
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
    border: 2rpx solid #eee;
}

.card_list_head_center {
    font-size: 36rpx;
    color: #232323;
    margin-left: 20rpx;
    flex: 1;
}

.card_list_head_center text {
    font-size: 24rpx;
    color: #969696;
    margin-left: 8rpx;
}

.card_list_head_right {
    width: 54rpx;
    height: 54rpx;
}

.card_list_content {
    padding: 24rpx;
    position: relative;
}

.card_list_content_single {
    font-size: 24rpx;
    color: #969696;
    margin-bottom: 20rpx;
}

.card_list_content_single text {
    font-family: Arial,Helvetica,sans-serif;
    color: #232323;
    font-size: 24rpx;
    margin-left: 24rpx;
}

.card_list_content_single:last-child {
    margin-bottom: 0;
}

.card_list_link {
    width: 54rpx;
    height: 54rpx;
    border-radius: 50%;
    background-color: #2F74FD;
    position: absolute;
    bottom: 24rpx;
    right: 24rpx;
}

.card_list_link image {
    position: absolute;
    margin: auto;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    width: 12rpx;
    height: 21rpx;
}