page {
    color: #454545;
    font-size: 28rpx;
}

view {
    overflow: visible;
}

image {
    overflow: visible;
}

.b-fff {
    background: #ffffff;
}

.mb20 {
    margin-bottom: 20rpx;
}

.mr10 {
    margin-right: 10rpx;
}

.pd10 {
    padding: 10rpx;
}

.flex-row {
    display: flex;
    flex-direction: row;
}

.flex-shu-center {
    align-items: center;
}

.color_red {
    color: #f03150;
}

input {
    border: 0;
    padding: 0;
    margin: 0;
}

.f-bold {
    font-weight: bold;
}

.nav {
    border-bottom: 2rpx solid #eeeeee;
    border-top: 2rpx solid #eeeeee;
    background: #ffffff;
    height: 94rpx;
    line-height: 94rpx;
}

.nav>view {
    width: 25%;
    text-align: center;
}

.nav_sub {
    height: 88rpx;
    line-height: 88rpx;
    display: inline-block;
}

.list {
    background: #fff;
    height: 140rpx;
    flex-direction: row;
    display: flex;
    align-items: center;
    padding: 0 20rpx;
    border-bottom: 2rpx solid #eee;
}

.list:first-child {
    margin-top: 20rpx;
}

.list>view:first-child {
    color: #999;
}

.list>view:nth-child(2) {
    margin: 0 32rpx 0 26rpx;
}

.status {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    color: #fff;
    height: 50rpx;
    border-radius: 10rpx;
    width: 100rpx;
    font-size: 24rpx;
}