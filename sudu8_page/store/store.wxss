.toubu {
    position: fixed;
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    top: 0;
    box-sizing: border-box;
    z-index: 1000;
    padding: 0 2%;
    background-color: #fff;
}

.toubu .qiehuan {
    color: #bbb;
    display: inline;
}

.toubu_r {
    float: right;
}

.toubu_r span {
    color: #f38539;
}

.list_store {
    border-bottom: 1px solid #eee;
    padding: 20rpx;
    height: 140rpx;
    overflow: hidden;
}

.thumb {
    float: left;
}

.thumb image {
    width: 140rpx;
    height: 140rpx;
    display: block;
    border-radius: 90rpx;
    border: 2rpx solid #eee;
    padding: 2rpx;
}

.title {
    float: left;
    width: 50%;
    margin-left: 20rpx;
    padding-top: 2rpx;
}

.biaots {
    font-size: 28rpx;
    color: #000;
}

.ccls {
    font-size: 24rpx;
    color: #888;
    margin-top: 10rpx;
    height: 40rpx;
    overflow: hidden;
}

.ccls image {
    width: 28rpx;
    height: 28rpx;
    float: left;
    opacity: 0.6;
    margin: 5rpx 6rpx 0 0;
}

.dianh {
    float: right;
    vertical-align: top;
    text-align: center;
    width: 20%;
    padding-top: 10rpx;
}

.dianh image {
    width: 70rpx;
    height: 70rpx;
}

.shukd {
    font-size: 24rpx;
    padding-top: 8rpx;
}