page {
    background-color: #FFF;
}

.hbj {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.hbj2 {
    display: flex;
    flex-direction: row;
}

.flex1 {
    flex: 1;
}

.jflqbanner {
    width: 100%;
    height: 331rpx;
    display: block;
    position: relative;
}

.integral_collectbox {
    margin-top: 20rpx;
    padding: 0 30rpx;
}

.integral_single {
    padding: 45rpx 0;
    border-bottom: 2rpx dashed #eeeeee;
}

.px {
    width: 47rpx;
    height: 45rpx;
    display: block;
    position: relative;
}

.px view {
    width: 47rpx;
    height: 45rpx;
    position: absolute;
    left: 0;
    top: 0;
    line-height: 45rpx;
    text-align: center;
    font-size: 24rpx;
    font-family: Arial;
    color: #fff;
}

.integral_title {
    font-size: 30rpx;
    color: #434343;
    margin-left: 12rpx;
    width: 420rpx;
    height: 44rpx;
    line-height: 44rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.integral_jf {
    font-size: 30rpx;
    color: #f85a46;
    font-family: Arial;
}

.integral_single-view2 {
    margin-top: 10rpx;
}

.integral_intro {
    width: 479rpx;
    height: 100rpx;
    line-height: 50rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}

.integral_btn {
    font-size: 30rpx;
    color: #fff;
    width: 146rpx;
    height: 52rpx;
    line-height: 52rpx;
    background: linear-gradient(to right,#F63F45 0%,#f96146 100%);
    text-align: center;
    border-radius: 52rpx;
}