.container {
    position: relative;
    width: 100%;
    height: 100%;
    background-color: #fff;
    color: #939393;
}

.nav_left {
    display: inline-block;
    width: 25%;
    height: 100%;
    background: #f5f5f5;
    text-align: center;
}

.nav_left .nav_left_items {
    height: 40px;
    line-height: 40px;
    padding: 6px 0;
    border-bottom: 1px solid #dedede;
    font-size: 14px;
}

.nav_left .nav_left_items.active {
    background: #fff;
    color: #f0145a;
}

.nav_right {
    position: absolute;
    top: 0;
    right: 0;
    flex: 1;
    width: 75%;
    padding: 0 10px;
    box-sizing: border-box;
    background: #fff;
}

.nav_right_t {
    height: 40px;
    line-height: 40px;
    padding: 6px 0;
    font-size: 14px;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.nav_right .nav_right_items {
    float: left;
    width: 33.33%;
    text-align: center;
    margin-bottom: 15px;
}

.nav_right .nav_right_items text {
    display: block;
    font-size: 14px;
    color: black;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.nodata_text {
    color: black;
    font-size: 14px;
    text-align: center;
}