page {
    color: #333;
    font-size: 28rpx;
}

image {
    overflow: visible;
}

.b-fff {
    background: #ffffff;
}

.mb20 {
    margin-bottom: 20rpx;
}

.mr10 {
    margin-right: 10rpx;
}

.pd10 {
    padding: 10rpx;
}

.flex-row {
    display: flex;
    flex-direction: row;
}

.flex-shu-center {
    align-items: center;
}

.color_red {
    color: #f03150;
}

input {
    border: 0;
    padding: 0;
    margin: 0;
}

.f-bold {
    font-weight: bold;
}

.main>view {
    padding: 20rpx;
}

.main>view:nth-child(1) {
    justify-content: space-between;
    border-bottom: 2rpx solid #eeeeee;
}

.main>view:nth-child(1) view {
    display: flex;
    flex-direction: column;
    justify-content: center;
}

.main>view:nth-child(2) {
    border-bottom: 2rpx solid #eeeeee;
}

.main>view.sub {
    width: 690rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    color: #ffffff;
    background: #ff4546;
    margin: 60rpx auto 0;
    border-radius: 10rpx;
    padding: 0;
}

.ti_all {
    font-size: 22rpx;
    background: green;
    height: 60rpx;
    color: #ffffff;
    padding: 0 10rpx;
    border-radius: 10rpx;
}