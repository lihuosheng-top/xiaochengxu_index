view {
    overflow: visible;
}

page {
    color: #454545;
    font-size: 28rpx;
}

image {
    overflow: visible;
}

.b-fff {
    background: #ffffff;
}

.mb20 {
    margin-bottom: 20rpx;
}

.mr10 {
    margin-right: 10rpx;
}

.pd10 {
    padding: 10rpx;
}

.flex-row {
    display: flex;
    flex-direction: row;
}

.flex-shu-center {
    align-items: center;
}

.color_red {
    color: #f03150;
}

input {
    border: 0;
    padding: 0;
    margin: 0;
}

.f-bold {
    font-weight: bold;
}

.nav {
    border-bottom: 2rpx solid #eeeeee;
    border-top: 2rpx solid #eeeeee;
    background: #ffffff;
    height: 94rpx;
    line-height: 94rpx;
}

.nav>view {
    width: 50%;
    text-align: center;
}

.nav_sub {
    height: 88rpx;
    line-height: 88rpx;
    display: inline-block;
}

.list {
    background: #fff;
    padding: 30rpx 20rpx;
    border-bottom: 2rpx solid #dcdcdc;
    align-items: center;
    justify-content: space-between;
    color: #000000;
}

.list:first-child {
    margin-top: 20rpx;
}

d_v1 .list:last-child {
    border-bottom: 0;
}

.list>view:first-child {
    align-items: center;
    width: 540rpx;
    border-right: 2rpx solid #eee;
}

.list_r {
    text-align: right;
}

.time {
    font-size: 24rpx;
    color: #979797;
}