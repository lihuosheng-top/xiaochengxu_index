.order_detail_head {
    width: 100%;
    height: 140rpx;
    display: block;
    position: relative;
}

.order_detail_head view {
    width: 100%;
    height: 140rpx;
    text-align: center;
    line-height: 140rpx;
    font-size: 32rpx;
    color: #fff;
    position: absolute;
    left: 0;
    top: 0;
}

.order_detial_person_info {
    padding: 30rpx;
    background-color: #fff;
}

.order_detail_address_img {
    width: 40rpx;
    height: 60rpx;
    overflow: hidden;
    font-size: 40rpx;
}

.person_info_left {
    margin-left: 24rpx;
}

.recive_person {
    font-size: 24rpx;
    color: #636363;
}

.recive_phonenum {
    font-size: 24rpx;
    color: #636363;
    font-family: Arial,Helvetica,sans-serif;
}

.recive_address {
    font-size: 24rpx;
    color: #636363;
    line-height: 40rpx;
    margin-top: 10rpx;
}

.pricebox {
    border-top: 2rpx solid #eee;
    padding: 20rpx 30rpx;
    background-color: #fff;
}

.price_single {
    height: 60rpx;
    line-height: 60rpx;
    overflow: hidden;
}

.price_single_left {
    font-size: 24rpx;
    color: #969696;
}

.price_single_right {
    font-size: 24rpx;
    color: #969696;
    line-height: 60rpx;
}

.price_single_left2 {
    font-size: 28rpx;
    color: #232323;
}

.price_single_left2 text {
    font-size: 24rpx;
    color: #969696;
}

.price_single_right2 {
    font-size: 36rpx;
    color: #f45351;
}

.price_single_right2 text {
    font-size: 24rpx;
}

.order_info {
    padding: 20rpx 30rpx;
    background-color: #fff;
}

.order_info_ddh_left {
    font-size: 24rpx;
    color: #969696;
}

.order_info_ddh_left text {
    font-family: Arial,Helvetica,sans-serif;
}

.copy_btn {
    width: 100rpx;
    height: 40rpx;
    border: 2rpx solid #E5E5E5;
    border-radius: 6rpx;
    font-size: 24rpx;
    color: #969696;
    text-align: center;
    line-height: 40rpx;
}

.smfw {
    height: 80rpx;
    background-color: #fff;
    padding: 0 30rpx;
}

.smfw_left {
    font-size: 24rpx;
    color: #232323;
}

.smfw_left text {
    font-family: Arial,Helvetica,sans-serif;
}

.appoint_form {
    background-color: #fff;
}

.appoint_form_head {
    padding: 0 30rpx;
    height: 85rpx;
    border-bottom: 2rpx solid #EEEEEE;
}

.appoint_form_left {
    font-size: 28rpx;
    color: #232323;
}

.appoint_form_right {
    font-size: 24rpx;
    color: #f45351;
}

.appoint_form_singlebox {
    padding: 20rpx 30rpx;
}

.appoint_form_singlebox input {
    border: none;
    overflow: hidden;
    padding: 0;
    margin: 0;
}

.detail_imgbox {
    font-size: 0;
}

.detail_imgbox image {
    width: 120rpx;
    height: 120rpx;
    display: inline-block;
    margin-right: 22rpx;
    margin-bottom: 22rpx;
    border-radius: 8rpx;
}

.detail_imgbox image:nth-child(5n) {
    margin-right: 0;
}

.detail_bz {
    font-size: 24rpx;
    color: #232323;
    background-color: #fff;
    padding: 0 30rpx;
    height: 80rpx;
    line-height: 80rpx;
    overflow: hidden;
}

.detail_bz text {
    margin: 4rpx 15rpx 0 0;
    color: #f45351;
}

.detail_btnbox {
    height: 112rpx;
    background-color: #fff;
    padding: 0 30rpx;
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    box-sizing: border-box;
}

.detail_btn {
    width: 160rpx;
    height: 50rpx;
    line-height: 46rpx;
    border-radius: 8rpx;
    text-align: center;
    border: 2rpx solid #E5E5E5;
    font-size: 24rpx;
    color: #636363;
    margin-left: 20rpx;
    box-sizing: border-box;
    padding: 0;
}

.hx_con {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: 99999;
}

.hexiao {
    border-radius: 9rpx;
    width: 600rpx;
    height: 300rpx;
    background: #fff;
    position: absolute;
    left: 75rpx;
    top: 30%;
    z-index: 99999;
}

.hexiao .hx_tit {
    margin: 20rpx 20rpx 10rpx;
    text-align: center;
}

.hexiao .hx_ipt {
    margin: 20rpx;
    height: 30rpx;
    line-height: 30rpx;
    border: 1px solid #eee;
}

.hexiao .hx_btn {
    background-color: #f90;
    color: #fff;
    margin: 20rpx;
    margin-top: 0rpx;
    margin-bottom: 20rpx;
    font-size: 28rpx;
}

.hx_c {
    width: 50rpx;
    height: 50rpx;
    position: absolute;
    top: 700rpx;
    left: 350rpx;
    z-index: 99999;
}

.hx_c image {
    width: 100%;
    height: 100%;
    display: block;
}

.fill_info {
    position: fixed;
    left: 20%;
    width: 60%;
    background-color: #5CB85C;
    border-radius: 10rpx;
    padding: 20rpx;
    top: 300rpx;
    z-index: 1000;
}

.fillinfo_cancel {
    text-align: center;
    color: #fff;
    margin-top: 20rpx;
    width: 45%;
    height: 60rpx;
    line-height: 60rpx;
    border-radius: 5rpx;
    background-color: green;
}

.fillinfo_submit {
    text-align: center;
    color: #fff;
    width: 45%;
    margin-left: 10%;
    margin-top: 20rpx;
    height: 60rpx;
    line-height: 60rpx;
    border-radius: 5rpx;
    background-color: green;
}